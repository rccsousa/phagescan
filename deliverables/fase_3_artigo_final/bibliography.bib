doi = {https://doi.org/10.1016/B978-0-12-823521-8.00006-2},
url = {https://www.sciencedirect.com/science/article/pii/B9780128235218000062},
author = {Gilberto Francisco Martha {de Souza} and Adherbal {Caminada Netto} and Arthur Henrique {de Andrade Melani} and Miguel Angelo {de Carvalho Michalski} and Renan Favarão {da Silva}},
keywords = {Engineering fault diagnosis, Fault detection and diagnosis, FDD, Engineering maintenance management, Condition-based maintenance, Naïve Bayes classifier, Model-based fault detection},
abstract = {This chapter focuses on fault diagnosis, a reasoning process that has gained the attention of operators and maintainers of engineering systems due to its ability to assist in maintenance management decision-making. The first section of the chapter presents the basic concepts of fault diagnosis and contextualizes them in an increasingly complex industrial sector. In the second section, the main methods currently used to solve diagnostic problems are discussed. The third section examines the major limitations of fault diagnosis, along with the main desirable characteristics of a fault detection and diagnosis (FDD) system. Finally, the fourth section presents the steps to be followed when developing a diagnostic system and shows how to choose the best method to be implemented.}
}

