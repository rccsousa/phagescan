title = {Chapter 6 - Engineering systems' fault diagnosis methods},
editor = {Gilberto Francisco Martha {de Souza} and Adherbal {Caminada Netto} and Arthur Henrique {de Andrade Melani} and Miguel Angelo {de Carvalho Michalski} and Renan Favarão {da Silva}},
booktitle = {Reliability Analysis and Asset Management of Engineering Systems},
